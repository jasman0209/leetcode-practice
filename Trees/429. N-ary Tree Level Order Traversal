/*
// Definition for a Node.
class Node {
public:
    int val;
    vector<Node*> children;

    Node() {}

    Node(int _val) {
        val = _val;
    }

    Node(int _val, vector<Node*> _children) {
        val = _val;
        children = _children;
    }
};
*/

class Solution {
public:
    
    vector<vector<int>> levelOrder(Node* root) {
        if(root==NULL) return {};
        vector<vector<int>> v;
        queue<Node *>q;
        q.push(root);
        while(!q.empty())
        {
            int n=q.size();
            vector<int>v1;
            
            for(int i=0;i<n;i++)
            {
                Node *fro=q.front();
                q.pop();
                v1.push_back(fro->val);
                for(int j=0;j<fro->children.size();j++)
                {
                    q.push(fro->children[j]);
                }
                
            }
            v.push_back(v1);
        }
        return v;
    }
};
