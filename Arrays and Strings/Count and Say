class Solution {
public:
    string countAndSay(int n) {
        if(n==0) return "";
        string s="1";
        while(--n)
        {
            
            string res="";
            for (int i = 0; i < s.size(); i++) 
            {int count=1;
                while(i<s.size()&&s[i]==s[i+1])
                    {count++;i++;}
                res=res+to_string(count)+s[i];
                
            }
            s=res;
        }
        return s;
    }
};
