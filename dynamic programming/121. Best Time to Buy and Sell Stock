#include<bits/stdc++.h> 
using namespace std; 
class Solution {
public:
    int maxProfit(vector<int>& prices) {
        
        if(prices.size()==0) return 0;
        int cp=prices[0];
        int i=1;
        int profit=0;
        while(i<prices.size()){
            if(cp>prices[i]){
                cp=prices[i];
            }
            else{
               if(prices[i]-cp>profit){
                   profit=prices[i]-cp;
               }
            }i=i+1;
        }return profit;}
};
