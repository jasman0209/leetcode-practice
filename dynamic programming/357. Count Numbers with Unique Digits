class Solution {
public:
    int countNumbersWithUniqueDigits(int n) {
        int dp[n+1];
        
        if(n==0) return 1;
        if(n==1) return 10;
        dp[0]=1;
        dp[1]=10;
        dp[2]=91;
        if(n<=2) return dp[n];
        for(int i=2;i<=n;i++) dp[i]=9;
        for(int i=2;i<=n;i++)
        {
            for(int j=2;j<=i;j++)
            {
                dp[i]=dp[i]*(9-j+2);
            }
            
                dp[i]=dp[i]+dp[i-1];
            
            //cout<<dp[i]<<" aa "<<endl;
        }
        
        return dp[n];
        
    }
};
